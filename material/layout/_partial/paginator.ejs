<% if (page.total > 1){ %>
  <div class="container">
    <div class="row">
      <div class="col s12 l8 offset-l2">
        <div class="paginator valign-wrapper">
          <%- paginator({
            prev_text: "<i class=\"material-icons\">chevron_left</i>",
            next_text: "<i class=\"material-icons\">chevron_right</i>"
          }) %>
        </div>
      </div>
    </div>
  </div>
<% } %>
